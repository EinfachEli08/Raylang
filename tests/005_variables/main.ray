// importing putchar from gcc
extern putchar

// ending the line
func endLine() => putchar(10)

// nested testing
func putCharacter(c) => putchar(c);

// printing the variable
func printVars(in, another) {
    // accessing the variables
    putCharacter(in)
    putCharacter(another)

    //returning the variable
    //return(in)
}

// Call both and return success
func main() {
    var myVar, yourVar

    myVar = 42
    yourVar = 69

    // should set giveVar to the value of myVar
    printVars(myVar, yourVar)
    putchar(myVar)
    endLine()

    //return should be 42
    return(myVar)
}