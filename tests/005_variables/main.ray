// importing putchar from gcc
extern putchar

// ending the line
func endLine() => putchar(10)

// nested testing
func putCharacter(c) => putchar(c);

// printing the variable
func printVars(in, another) {
    // accessing the variables
    putCharacter(in)
    putCharacter(another)

    //returning the variable
    //return(in)
}

func getVar(input) {
    // this function is not used in the main function
    return input
}

// Call both and return success
func main() {
    var myVar, yourVar

    myVar = 42
    yourVar = 69

    //testing variable assignment
    var anotherVar = myVar
    var star
    star = getVar(anotherVar)
    // should set giveVar to the value of myVar
    printVars(myVar, yourVar)
    putchar(myVar)
    endLine()

    putchar(anotherVar)
    putchar(star)
    endLine()

    //return should be 42
    return(myVar)
}