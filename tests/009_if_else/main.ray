extern putchar

func main(){
    var compare = 3

    // comparing agains a value
    if(compare == 3){

        // checking if true
        if(true) endl()

        // comparing against a boolean
        if(false == true) putchar(69) else putchar(compare)

        // comparing a boolean against a number
        if(false == 0) { 
            endl() 
        }
    } else {
        exit(69)
    }
}

func endl() => putchar(10)
